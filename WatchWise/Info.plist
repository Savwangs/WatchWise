	<array>
		<string>remote-notification</string>
		<string>processing</string>
		<string>background-fetch</string>
	</array>
	<key>BGTaskSchedulerPermittedIdentifiers</key>
	<array>
		<string>com.watchwise.heartbeat</string>
		<string>com.watchwise.monitoring</string>
		<string>com.watchwise.deviceactivity</string>
		<string>com.watchwise.dailyreset</string>
